[Rewrite]
# 谷歌域名重定向
^https?:\/\/(ditu|maps).google\.cn https://maps.google.com 302
^https?:\/\/(www.)?(g|google)\.cn https://www.google.com 302

# NGA重定向
^https?:\/\/bbs\.nga\.cn https://ngabbs.com 302
^https?:\/\/nga\.178\.com https://ngabbs.com 302
^http:\/\/ngabbs\.com https://ngabbs.com 302

# NGA去广告
^https:\/\/pgdt\.ugdtimg\.com - reject-dict
^https:\/\/g1\.tagtic\.cn - reject-dict

# 南+重定向
^https?:\/\/((www|bbs)\.)?(north|spring|white)-plus\.(net|org)/(.*) https://south-plus.net/$1 302

[MitM]
hostname = bbs.nga.cn, nga.178.com, google.cn, www.google.cn, ditu.google.cn, map.google.cn, www.g.cn, *.north-plus.net, *.spring-plus.net, *.white-plus.net, *.north-plus.org, *.spring-plus.org, *.white-plus.org, north-plus.net, spring-plus.net, white-plus.net, pgdt.ugdtimg.com, g1.tagtic.cn
h2 = true
